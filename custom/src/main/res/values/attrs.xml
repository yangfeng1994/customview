<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StageProgressView">
        <attr name="play_progress_bar_item_interval" format="float|dimension" />
        <attr name="play_progress_progress_margin_top" format="float|dimension" />
        <attr name="play_progress_progress_height" format="float|dimension" />
        <attr name="play_progress_min_text_margin_top" format="float|dimension" />
        <attr name="play_progress_min_text_size" format="float|dimension" />
        <attr name="play_progress_icon_height" format="float|dimension" />
        <attr name="play_progress_icon_width" format="float|dimension" />
        <attr name="play_progress_complete_progress_color" format="color" />
        <attr name="play_progress_in_progress_color" format="color" />
        <attr name="play_progress_default_progress_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomChronometer">
        <attr name="format" format="string" localization="suggested" />
        <attr name="countDown" format="boolean" />
        <attr name="downEndText" format="string"/>
    </declare-styleable>
</resources>
